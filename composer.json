{
  "name": "ronasit/laravel-media",
  "description": "A module for working with Media.",
  "keywords": [
    "framework",
    "laravel",
    "media"
  ],
  "license": "MIT",
  "authors": [
      {
          "name": "Ronas IT",
          "homepage": "https://github.com/RonasIT/laravel-media/graphs/contributors"
      }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": ">=11.44.1",
    "ronasit/laravel-helpers": ">=3.4",
    "spatie/laravel-medialibrary": "^11.8.3",
    "bepsvpt/blurhash": ">=3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5.30",
    "orchestra/testbench": "^9.3",
    "php-coveralls/php-coveralls": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "RonasIT\\Media\\": "src/"
    },
    "exclude-from-classmap": [
      "tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "RonasIT\\Media\\Tests\\": "tests/",
      "RonasIT\\Media\\Tests\\Support\\": "tests/Support/"
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "RonasIT\\Media\\MediaServiceProvider"
          ]
      }
  }
}
